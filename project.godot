[animation]

compatibility/default_parent_skeleton_in_mesh_instance_3d=true

[application]

config/name="Cam Hytale ANE (Asset Node Editor)"
config/description="[Beta] A custom, cross-platform Asset Node editor for hytale asset nodes, primarily for worldgen v2"
config/version="0.4.2"
run/main_scene="res://graph_editor/standalone_editor.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="CamHytaleANE"
config/features=PackedStringArray("4.6", "GL Compatibility")
run/low_processor_mode=true
config/icon="uid://dm1qflfglyyv7"

[autoload]

ANESettings="*uid://js4qrto53qyd"
Util="*res://global/util.gd"
SchemaManager="*uid://dadtbq1o8e5kp"
